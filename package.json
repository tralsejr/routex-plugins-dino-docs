    "release:minor": "standard-version --release-as minor && git push --follow-tags origin main",
    "release:major": "standard-version --release-as major && git push --follow-tags origin main"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tralsejr/routex-plugins-dino-docs.git"
  },
  "author": "Gio Kent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tralsejr/routex-plugins-dino-docs/issues"
  },
  "homepage": "https://github.com/tralsejr/routex-plugins-dino-docs#readme",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "standard-version": "^9.5.0",
    "supertest": "^7.0.0",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@tralsejr/dino-docs-api": "^0.0.2",
    "@tralsejr/routex": "^0.0.3",
    "express-validator": "^7.1.0"
