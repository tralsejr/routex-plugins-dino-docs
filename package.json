{
  "name": "@tralsejr/routex-plugins-dino-docs",
  "version": "0.0.2",
  "description": "DinoDoc plugin for routex.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "prestart": "npm test",
    "build": "tsup",
    "pub": "tsup && npm publish --access=public && exit",
